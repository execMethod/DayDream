<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android"
  >
  <item>
    <shape
      android:shape="rectangle">
      <corners
        android:radius="3dp"
      />
      <solid
        android:color="@color/none"
      />
      <stroke
        android:color="@color/white"
        android:width="1dp"
      />
    </shape>
  </item>
  <item>
    <bitmap
      android:gravity="center"
      android:src="@drawable/cog_src"
    />
  </item>
</layer-list>

